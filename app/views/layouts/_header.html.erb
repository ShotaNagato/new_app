<header class="navbar">
    <div class="container"> 
     <%= link_to image_tag("twitter_header_photo_1.png",width:"250px",height:"100px"),root_path ,id:"logo" %>
     <nav>
      <ul class="nav">
       <li><%= link_to 'アプリ詳細', about_path,id:"ab" %></li>
       <li><%= link_to 'ヘルプ', help_path,id:"ab" %></li>
       <% if s_logged_in? %>
        <li><%= link_to "先生一覧", t_users_path ,id:"ab" %></li>
        <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              <div class="account">アカウント</div ><b class="caret"></b>
            </a>
            <ul class="dropdown-menu">
              <li><%= link_to "プロフィール", current_s_user,id:"ab" %></li>
              <li><%= link_to "登録内容変更", edit_s_user_path(current_s_user),id:"ab" %></li>
              <li class="divider"></li>
              <li>
                <%= link_to "ログアウト", logout_path, method: :delete,id:"ab" %>
              </li>
            </ul>
        </li>
       <% elsif t_logged_in? %>
        <li><%= link_to "生徒一覧", s_users_path,id:"ab" %></li>
        <li class="dropdown">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              <div class="account">アカウント</div> <b class="caret"></b>
            </a>
            <ul class="dropdown-menu">
              <li><%= link_to "プロフィール", current_t_user,id:"ab" %></li>
              <li><%= link_to "登録内容変更", edit_t_user_path(current_t_user),id:"ab" %></li>
              <li class="divider"></li>
              <li>
                <%= link_to "ログアウト", logout_path, method: :delete,id:"ab" %>
              </li>
            </ul>
          </li>
         <% else %>
      <li><%= link_to "ログイン", login_path,id:"ab" %></li>
      <% end %>
      </ul> 
     </nav>
    </div>
</header>
